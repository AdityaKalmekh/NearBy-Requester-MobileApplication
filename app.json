{
  "expo": {
    "name": "NearBy",
    "slug": "NearByRequester",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/nearby-1024.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugins": [
      "expo-dev-client",
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app needs access to location to help you find nearby service providers.",
          "locationAlwaysPermission": "This app needs access to location to help you find nearby service providers.",
          "locationWhenInUsePermission": "This app needs access to location to help you find nearby service providers.",
          "isIosBackgroundLocationEnabled": false,
          "isAndroidBackgroundLocationEnabled": false
        }
      ]
    ],
    "developmentClient": {
      "silentLaunch": false
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/6f4a6e3c-6369-4d41-aa34-b0d96ef40f39"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nearby.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to help you find nearby service providers.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to location to help you find nearby service providers.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/nearby-1024.png",
        "backgroundColor": "#000000"
      },
      "package": "com.nearby.app",
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "INTERNET",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/nearby-favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6f4a6e3c-6369-4d41-aa34-b0d96ef40f39"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
