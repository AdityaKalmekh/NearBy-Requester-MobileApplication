{
  "name": "nearbyrequester",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:dev": "EXPO_PUBLIC_ENV=development expo start",
    "start:prod": "EXPO_PUBLIC_ENV=production expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:development": "eas build --profile development --platform android",
    "build:preview": "eas build --profile preview --platform android",
    "build:production": "eas build --profile production --platform android",
    "build:all-dev": "eas build --profile development --platform all",
    "build:all-preview": "eas build --profile preview --platform all",
    "build:all-production": "eas build --profile production --platform all",
    "update:development": "eas update --branch development",
    "update:preview": "eas update --branch preview",
    "update:production": "eas update --branch production",
    "update:auto-dev": "eas update --auto --branch development",
    "update:auto-preview": "eas update --auto --branch preview",
    "update:auto-prod": "eas update --auto --branch production"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@react-navigation/bottom-tabs": "^7.3.13",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/stack": "^7.3.2",
    "axios": "^1.9.0",
    "expo": "~53.0.9",
    "expo-constants": "^17.1.6",
    "expo-dev-client": "~5.1.8",
    "expo-location": "^18.1.5",
    "expo-secure-store": "^14.2.3",
    "expo-splash-screen": "^0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.13",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-maps": "1.20.1",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0",
    "react-native-svg": "15.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "private": true
}